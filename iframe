<script type="text/javascript">
(function(){
var style = document.createElement('style'),
head = document.head || document.getElementsByTagName("head")[0];;
style.type = 'text/css';
head.appendChild(style);
var query2object = function(str){
var obj = {},
queries = (str||'').split('&');
for(var i = 0; i < queries.length; i++){
var item = queries[i],
temp = item.split('=');
if (!temp||!temp.length) return;
var key = temp.shift();
if (!key) return;
obj[decodeURIComponent(key)] = decodeURIComponent(temp.join('='));
}
return obj;
};
var addCss = function(selector, attr, value){
if(!selector || !value) return;
// auto add '#'
value = '#' + value;
if(!attr){
var cssText = selector+'{'+value+'}';
}else{
cssText = selector+'{'+attr+':'+value+'}';
}
if (style.styleSheet) { // for IE
style.styleSheet.cssText += cssText;
}else{
var textNode = document.createTextNode(cssText);
style.appendChild(textNode);
}
};
var bgColorDisabled = "-moz-opacity:0.6;-khtml-opacity: 0.6;opacity: 0.6;",
bgColorActive = "-moz-opacity:0.6;-khtml-opacity: 0.6;opacity: 0.6;",
bgColorHover = "-moz-opacity:0.8;-khtml-opacity: 0.8;opacity: 0.8;";
var tagReg = /<\/?[^>]*>/g,
search = location.search.replace(tagReg, '');
if(search){
search = query2object(location.search.substr(1));
}
var topbarBgColor = search['topbarBgColor'],
chatPanelBgColor = search['chatPanelBgColor'],
editorColor = search['editorColor'],
editorBgColor = search['editorBgColor'],
lMsgColor = search['lMsgColor'],
rMsgColor = search['rMsgColor'],
lMsgBgColor = search['lMsgBgColor'],
rMsgBgColor = search['rMsgBgColor'],
buttonColor = search['buttonColor'],
buttonBgColor = search['buttonBgColor'],
corpInfoColor = search['corpInfoColor'],
corpInfoTitleColor = search['corpInfoTitleColor'],
corpInfoBgColor = search['corpInfoBgColor'],
tipColor = search['tipColor'],
linkColor = search['linkColor'],
borderColor = search['borderColor'];
/*topbar*/
addCss('.g-hd', 'background', topbarBgColor);
/*chatPanel*/
addCss('.g-bd .bd_left', 'background', chatPanelBgColor);
// editor
addCss('.m-iptbox', 'background', editorBgColor);
addCss('.m-iptbox .textarea', 'background', editorBgColor);
/*corpInfo*/
addCss('.g-bd .bd_right', 'background', corpInfoBgColor);
//title
addCss('.m-announce .announce_tlt', 'color', corpInfoTitleColor);
addCss('.m-comquestions h1', 'color', corpInfoTitleColor);
//content
addCss('.m-announce .announce_cnt', 'color', corpInfoColor);
addCss('.m-comquestions .itm .title', 'color', corpInfoColor);
addCss('.m-comquestions .itm .answer', 'color', corpInfoColor);
/*left msg*/
// text
addCss('.m-msglist .msg_left .msg_wrapper', 'color', lMsgColor);
// background
addCss('.m-msglist .msg_left .msg_wrapper', 'background-color', lMsgBgColor);
addCss('.m-msglist .msg_left .crm-thirdorder-bd', 'background-color', lMsgBgColor);
addCss('.m-msglist .msg_left .crm-thirdorder-ft', 'background-color', lMsgBgColor);
// border
addCss('.m-msglist .msg_left .msg_wrapper', 'border-color', lMsgBgColor);
addCss('.m-msglist .msg_left .crm-thirdorder-bd', 'border-color', lMsgBgColor);
addCss('.m-msglist .msg_left .crm-thirdorder-ft', 'border-color', lMsgBgColor);
// arrow
addCss('.m-msglist .msg_left .msg_wrapper .arrow', 'border-right-color', lMsgBgColor);
addCss('.m-msglist .msg_left .msg_wrapper .arrow:before', 'border-right-color', lMsgBgColor);
// avator
addCss('.m-msglist .msg_left .u-icon-avater', 'background-color', lMsgBgColor);
/*right msg*/
// text
addCss('.m-msglist .msg_right .msg_wrapper', 'color', rMsgColor);
// background
addCss('.m-msglist .msg_right .msg_wrapper', 'background-color', rMsgBgColor);
// border
addCss('.m-msglist .msg_right .msg_wrapper', 'border-color', rMsgBgColor);
// arrow
addCss('.m-msglist .msg_right .msg_wrapper .arrow', 'border-left-color', rMsgBgColor);
addCss('.m-msglist .msg_right .msg_wrapper .arrow:before', 'border-left-color', rMsgBgColor);
// avator
addCss('.m-msglist .msg_right .u-icon-avater', 'background-color', rMsgBgColor);
/* modal */
addCss('.m-modal .modal_hd', 'border-bottom-color', buttonBgColor);
addCss('.m-modal .modal_ft', 'border-bottom-color', buttonBgColor);
addCss('.m-modal .ok', 'background-color', buttonBgColor);
addCss('.m-modal .ok:hover', 'background-color', buttonBgColor);
addCss('.m-modal .ok:active', 'background-color', buttonBgColor);
/*button*/
addCss('.u-btn, .u-btn:hover, .u-btn:link, .u-btn:active', 'background-color', buttonBgColor);
addCss('.u-btn:active', '', bgColorActive);
addCss('.u-btn.z-disabled, .u-btn.z-disabled:hover, .u-btn.z-disabled:link, .u-btn.z-disabled:active', '', bgColorDisabled);
addCss('.u-btn.z-disabled:hover', '', bgColorDisabled);
addCss('.u-btn.z-disabled:link', '', bgColorDisabled);
addCss('.u-btn.z-disabled:active', '', bgColorDisabled);
addCss('.u-btn-disabled, .u-btn-disabled:hover, .u-btn-disabled:active, .u-btn-disabled:link', '', bgColorDisabled);
addCss('.u-btn-disabled:hover', '', bgColorDisabled);
addCss('.u-btn-disabled:active', '', bgColorDisabled);
addCss('.u-btn-disabled:link', '', bgColorDisabled);
addCss('.z-disabled button,.z-disabled button:link,.z-disabled button:hover,.z-disabled button:active', 'background-color', buttonBgColor);
/*评价邀请*/
addCss('.m-comment .comment_btn, .m-comment .comment_btn:hover, .m-comment .comment_btn:active', 'background-color', buttonBgColor);
addCss('.m-comment .comment_btn:hover', '', bgColorHover);
addCss('.m-comment .comment_btn:active', '', bgColorActive);
/*botEntry*/
addCss('.m-botentry li', 'color', buttonBgColor);
addCss('.m-botentry li', 'border-color', buttonBgColor);
/*leave*/
addCss('.g-bd', 'background', chatPanelBgColor);
addCss('.m-leavemessage .message', 'color', buttonColor);
addCss('.m-form .formitm .lab', 'color', buttonColor);
/*scrollbar*/
//addCss('::-webkit-scrollbar-thumb', 'background', lMsgBgColor);
/*tips*/
addCss('.c-tip-text', 'color', tipColor);
addCss('.c-tip-bg', 'background-color', chatPanelBgColor);
addCss('.m-msglist .info h2', 'color', tipColor);
addCss('.m-msglist .info span', 'color', tipColor);
addCss('.m-msglist .info h2', 'background-color', chatPanelBgColor);
addCss('.m-msglist .info span', 'background-color', chatPanelBgColor);
addCss('.m-msglist .date span', 'background-color', chatPanelBgColor);
/*七鱼底部logo*/
//addCss('.g-td', 'background-color', chatPanelBgColor);
/*link*/
addCss('.g-bd a', 'color', linkColor);
addCss('.m-msglist a', 'color', linkColor);
addCss('.m-msglist a:hover', 'color', linkColor);
addCss('.m-msglist a:hover', '', bgColorHover);
addCss('.u-msg a:active, .u-msg a:link', 'color', linkColor);
/*border*/
addCss('.g-hd', 'border-bottom-color', borderColor);
addCss('.g-bd .bd_left', 'border-right-color', borderColor);
addCss('.m-iptbox', 'border-top-color', borderColor);
addCss('.m-announce .announce_tlt', 'border-bottom-color', borderColor);
addCss('.m-comquestions h1', 'border-bottom-color', borderColor);
addCss('.m-comquestions .itm', 'border-bottom-color', borderColor);
})();
</script>
