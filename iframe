{macro file(x)}
{var expired = !!(new Date().getTime() > x.content.expire)}
<div class="arrow"></div>
<div class="file-wrap {if !!expired}z-expired{/if}">
<div class="file-bd">
<div class="file-left">
<i class="u-icon-${x.content.name|spliceSuffix}"></i>
</div>
<div class="file-right">
<div class="hd">
<p class="name">${x.content.name}</p>
</div>
<div class="bd">
{if !isLeft}
{if x.state == 1}
<span class="progress-size size">${x|fileProgressFormat}</span>
<div class="progress-bar">
{var width = x.progressObj ? x.progressObj.percentage / 100 * 78 : 0}
<div class="inner" style="width: ${width + "px"}"></div>
</div>
{if x.uploadObj}
<i class="abort" data-action="abortFileTrans" data-id="${x.id}"></i>
{/if}
{else}
<span class="size">${x.content.size|fileSizeFormat}</span>
{if !!expired}
<span class="info">已失效</span>
{/if}
{/if}
{else}
<span class="size">${x.content.size|fileSizeFormat}</span>
{if !expired}
<a class="download" data-action="download" data-url="${x.content.url|formatDownloadUrl}" data-name="${x.content.name}">下载</a>
{else}
<span class="info">已失效</span>
{/if}
{/if}
</div>
</div>
</div>
<div class="file-ft">
{if x.state == -1}
<span class="error">网络连接失败</span>
<a class="resend" data-action="resend" data-id="${x.id}">重试</a>
{/if}
</div>
</div>
{/macro}
